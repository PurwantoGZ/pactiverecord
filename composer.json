{
  "name": "pactiverecord/pactiverecord",
  "type": "library",
  "description": "pactiverecord is extended library from phpactiverecord for support php 8.0 based on the ActiveRecord pattern.",
  "keywords": [
    "activerecord",
    "orm"
  ],
  "homepage": "https://github.com/PurwantoGZ/pactiverecord",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "ext-pdo": "*",
    "ext-xmlwriter": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.*",
    "squizlabs/php_codesniffer": "^3.5",
    "pear/pear_exception": "1.0-beta1",
    "pear/log": "~1.12"
  },
  "autoload": {
    "psr-4": {
      "ActiveRecord\\": "lib"
    },
    "files": [
      "lib/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "test"
    }
  }
}
